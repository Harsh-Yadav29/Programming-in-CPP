// Pattern17
/* A
   BC
   CDE
   
#include <iostream>
using namespace std;

int main() {
	int n;
	cin >> n;
	int row = 1;
	
	
	while(row <= n){
	    int col = 1;
	    char ch = 'A' + row - 1;
	    while(col <= row){
	        cout << ch;
	        ch = ch + 1;
	        col = col + 1;
	    }
	    cout << endl;
	    row = row + 1;
	}
	return 0;
}
*/

