// Pattern25
/*     1
      23
     456
		 
#include <iostream>
using namespace std;

int main() {
	int n;
	cin >> n;
	int row = 1;
	int count = 1;
	
	while(row <= n){
	    int col = 1;
	    while(col <= n){
	        if(col <= n-row){
	            cout << "  ";
	        }
	        else{
	            cout << count << " ";
	            count = count + 1;
	        }
	        col = col + 1;
	    }
	    cout << endl;
	    row = row + 1;
	}
	return 0;
}
*/

// Pattern26
/*     1
      121
     12321
    1234321
		
#include <iostream>
using namespace std;

int main() {
	int n;
	cin >> n;
	int row = 1;
	
	while(row <= n){
	    
	    int space = n-row;
	    while(space){
	        cout << " ";
	        space = space - 1;
	    }
	    
	    int col =  1;
	    while(col <= row){
	        cout << col;
	        col = col + 1;
	    }
	    
	    int start = row - 1;
	    while(start){
	        cout << start;
	        start = start - 1;
	    }
	    
	    cout << endl;
	    row = row + 1;
	}
	return 0;
}
*/

// Pattern27
/* 1234554321
   1234**4321
   123****321
   12******21
   1********1
   
#include <iostream>
using namespace std;

int main() {
	int n;
	cin >> n;
	int row = 1;
	
	while(row <= n){
	    int col = 1;
	    while(col <= n){
	        if(col <= n-row+1){
	            cout << col;
	        }
	        else{
	            cout << "*";
	        }
	        col = col + 1;
	    }
	    
	    int start = col - 1;
	    int star = 1;
	    while(start >= 1){
            if(star <= row-1){
                cout << "*";
                star = star + 1;
                start = start - 1;
            }
            else{
                cout << start;
                start = start - 1;
            }
	    }
	    cout << endl;
	    row = row + 1;
	}
	return 0;
}
*/
